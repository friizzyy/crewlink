{
  "name": "crewlink",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "postinstall": "prisma generate",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:qa": "playwright test tests/qa/ --project=chromium",
    "test:visual": "playwright test tests/visual/ --project=chromium",
    "test:perf": "playwright test tests/performance/ --project=chromium",
    "audit:perf": "npm run build && npx @lhci/cli autorun",
    "audit:security": "node scripts/security-scan.js",
    "audit:copy": "node scripts/copy-scan.js",
    "agents:all": "npm run test:qa && npm run test:visual && npm run audit:security && npm run audit:copy",
    "agents:quick": "npm run test:qa && npm run audit:security"
  },
  "dependencies": {
    "@mapbox/mapbox-gl-geocoder": "^5.0.2",
    "@prisma/client": "^5.10.0",
    "@types/leaflet": "^1.9.21",
    "clsx": "^2.1.0",
    "date-fns": "^3.3.1",
    "framer-motion": "^11.0.8",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.344.0",
    "mapbox-gl": "^3.2.0",
    "next": "^14.2.35",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-leaflet": "^4.2.1",
    "zod": "^3.22.4",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@lhci/cli": "^0.13.0",
    "@playwright/test": "^1.57.0",
    "@types/mapbox-gl": "^3.1.0",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.2.35",
    "postcss": "^8.4.0",
    "prisma": "^5.10.0",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
